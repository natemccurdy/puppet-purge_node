  "supports_noop": false,
  "parameters": {
    "agent_certnames": {
      "description": "A comma-separated list or JSON array of agents to purge",
      "type": "Variant[Pattern[/^([A-Za-z0-9._-]+,?)+$/], Array[Pattern[/^([A-Za-z0-9._-]+,?)+$/]]]"
    }
  }
}
